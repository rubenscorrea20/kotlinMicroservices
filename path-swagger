"/baas-payments-management/payments/accounts/{accountId}":
    post:
      tags:
        - payment-controller-impl
      summary: Criação de uma nova ordem de pagamento
      operationId: createCashOutPaymentUsingPOST
      consumes:
        - application/json
      produces:
        - application/json
      parameters:
        - name: accountId
          in: path
          description: Identificador de conta
          required: false
          type: string
        - in: body
          name: request
          description: Dados necessários para criação de uma nova ordem de pagamento
          required: false
          schema:
            $ref: "#/definitions/CashOutPaymentRequest"
        - name: x-baas-authorization
          in: header
          description: Token de Autenticação
          required: false
          type: string
        - name: x-tenant-id
          in: header
          description: Identificador da franquia
          required: false
          type: string
      responses:
        "201":
          description: Created
          schema:
            $ref: "#/definitions/CashOutPaymentResponse"
        "401":
          description: Unauthorized
        "403":
          description: Forbidden
        "404":
          description: Not Found
      deprecated: false
